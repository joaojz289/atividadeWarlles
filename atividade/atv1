#include <stdio.h>

int main() {
    int numero;

    printf("Digite um número de 5 dígitos: ");
    scanf("%d", &numero);

    
    if (numero >= 10000 && numero <= 99999) {
        
        int d1 = numero / 10000;
        int d2 = (numero / 1000) % 10;
        int d3 = (numero / 100) % 10;
        int d4 = (numero / 10) % 10;
        int d5 = numero % 10;

        printf("%d   %d   %d   %d   %d\n", d1, d2, d3, d4, d5);
    } else {
        printf("Número digitado incorretamente, digite novamente.\n");
    }

    return 0;
}

